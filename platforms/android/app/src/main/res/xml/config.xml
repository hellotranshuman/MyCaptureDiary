<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>MC Diary</name>
    <description>
        A sample capture diary
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="dist/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/MyCaptureDiary_icon32.png" />
    <icon density="mdpi" src="res/icon/android/MyCaptureDiary_icon48.png" />
    <icon density="hdpi" src="res/icon/android/MyCaptureDiary_icon72.png" />
    <icon density="xhdpi" src="res/icon/android/MyCaptureDiary_icon96.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
